{
  "name": "vampire-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev-win": "set NODE_ENV=development && run-p dev:1 dev:2",
    "dev": "NODE_ENV=development run-p dev:1 dev:2",
    "dev:1": "tsc --watch --preserveWatchOutput -p tsconfig.dev.json",
    "dev:2": "wait-on dev/main.js && nodemon -q --watch dev/ dev/main.js",
    "build": "run-s type-check build-only test",
    "build-only": "tsc",
    "type-check": "tsc --noEmit --composite false",
    "migrate:make": "knex migrate:make -x ts newmig",
    "run": "node dist/main.js",
    "test": "jest"
  },
  "dependencies": {
    "ajv-formats": "^2.1.1",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-oas-generator": "^1.0.46",
    "express-session": "^1.18.0",
    "filewatcher": "^3.0.1",
    "glob": "^10.3.12",
    "knex": "^2.5.1",
    "mysql2": "^3.9.7",
    "node-schedule": "^2.1.1",
    "objection": "^3.1.4",
    "sqlite3": "^5.1.7",
    "swagger2openapi": "^7.0.8"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@rushstack/eslint-patch": "^1.10.2",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jsdom": "^21.0.0",
    "@types/node": "^20.12.8",
    "@types/node-schedule": "^2.1.7",
    "@types/swagger2openapi": "^7.0.4",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "npm-run-all2": "^5.0.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5",
    "wait-on": "^7.2.0"
  }
}
